<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>1.3.0</Version>
    <PackageId>Bumblebee.Automation</PackageId>
    <Company>Patrick Herrmann, John Hammerlund, Todd Meinershagen</Company>
    <Copyright>Copyright Â©  2012</Copyright>
    <Description>Bumblebee is a .NET layer on top of the Selenium browser automation framework allowing for standardized creation of page objects, even for dynamic web pages.</Description>
    <Authors>Patrick Herrmann, John Hammerlund, Todd Meinershagen</Authors>
    <PackageLicenseUrl>https://github.com/Bumblebee/Bumblebee/blob/master/LICENSE.md</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/Bumblebee/Bumblebee</PackageProjectUrl>
    <PackageIconUrl>https://avatars1.githubusercontent.com/u/10162916?v=3&amp;s=100</PackageIconUrl>
    <RepositoryUrl>https://github.com/Bumblebee/Bumblebee</RepositoryUrl>
    <RepositoryType>GitHub</RepositoryType>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageTags>acceptance ui automation system test testing</PackageTags>
    <PackageReleaseNotes>Added support for .NET core projects by making project .NET Standard 2.0 compliant.</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Selenium.Support" Version="3.6.0" />
    <PackageReference Include="Selenium.WebDriver" Version="3.6.0" />
  </ItemGroup>

</Project>
